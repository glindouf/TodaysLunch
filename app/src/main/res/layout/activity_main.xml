<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.gustav.todayslunch.MainActivity">


    <ImageButton
        android:id="@+id/mimolett"
        android:layout_width="120dp"
        android:layout_height="59dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/logga_mimolett"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="46dp"

        />

    <ImageButton
        android:id="@+id/bistrot"
        android:layout_width="107dp"
        android:layout_height="87dp"
        android:layout_marginLeft="16dp"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toRightOf="@+id/mimolett"
        app:srcCompat="@drawable/bistrotlogga"
        tools:layout_editor_absoluteY="26dp" />

    <ImageButton
        android:id="@+id/lsExpress"
        android:layout_width="72dp"
        android:layout_height="103dp"
        android:layout_marginLeft="20dp"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toRightOf="@+id/bistrot"
        app:srcCompat="@drawable/ls_express_logga"
        tools:layout_editor_absoluteY="16dp" />

    <ImageButton
        android:id="@+id/matMinnen"
        android:layout_width="110dp"
        android:layout_height="72dp"
        android:layout_marginTop="28dp"
        android:scaleType="fitXY"
        app:layout_constraintTop_toBottomOf="@+id/mimolett"
        app:srcCompat="@drawable/mat_minnen"
        tools:layout_editor_absoluteX="16dp" />

</android.support.constraint.ConstraintLayout>
